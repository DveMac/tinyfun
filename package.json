{
  "name": "tinyfun",
  "version": "1.0.6",
  "description": "Tiny functional ES6 library",
  "main": "lib/index.js",
  "repository": {
    "url": "git@github.com:DveMac/tinyfun.git",
    "type": "git"
  },
  "scripts": {
    "test": "flow check && babel-node test.js | ./node_modules/.bin/faucet",
    "test:watch": "./node_modules/.bin/nodemon --watch ./ --ignore lib --exec 'npm test'",
    "prebuild": "mkdir -p lib ",
    "build": "NODE_ENV=production babel index.js > lib/index.js && cp ./index.js.flow lib/index.js.flow",
    "report": "npm run build && gzip -c lib/index.js | wc -c"
  },
  "keywords": [
    "functional",
    "library",
    "tiny",
    "small",
    "programming",
    "ES6",
    "compose",
    "reduce",
    "map"
  ],
  "author": "@dvemac",
  "license": "GPL",
  "dependencies": {
    "eslint-plugin-flowtype": "^2.32.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.2.2",
    "es6-minify": "ariya/es6-minify",
    "eslint": "^3.19.0",
    "faucet": "^0.0.1",
    "flow-bin": "^0.44.0",
    "nodemon": "^1.11.0",
    "tape": "^4.6.2"
  }
}
